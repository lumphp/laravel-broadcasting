{
    "name": "lumphp/laravel-broadcasting",
    "description": "The Illuminate Broadcasting package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-broadcasting/issues",
        "source": "https://github.com/lumphp/laravel-broadcasting"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "psr/log": "^1.0",
        "lumphp/laravel-bus": "^8.0",
        "lumphp/laravel-collections": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-queue": "^8.0",
        "lumphp/laravel-support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Broadcasting\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "pusher/pusher-php-server": "Required to use the Pusher broadcast driver (^4.0|^5.0|^6.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
