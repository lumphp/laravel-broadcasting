{
    "name": "lumphp/laravel-broadcasting",
    "description": "The Illuminate Broadcasting package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-broadcasting/issues",
        "source": "https://github.com/lumphp/laravel-broadcasting"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "lumphp/laravel-contracts": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^5.1.4)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
